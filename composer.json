{
    "name": "kyranb/footprints",
    "type": "library",
    "description": "A simple registration attribution tracking solution for Laravel 5.2+ (UTM Parameters and Referrers)",
    "keywords": [
        "footprints",
        "tracking",
        "utm",
        "utm parameters",
        "user tracking",
        "attribution tracking"
    ],
    "homepage": "https://github.com/kyranb/footprints",
    "license": "MIT",
    "authors": [
        {
            "name": "Kyran Beasley",
            "email": "kyranbeasley@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.*|6.*|7.*",
        "php" : "~5.5|7.*"
    },
    "require-dev": {
        "laravel/framework": "5.*",
        "phpunit/phpunit" : "4.*",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Kyranb\\Footprints\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kyranb\\Footprints\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Kyranb\\Footprints\\FootprintsServiceProvider"
            ],
            "aliases": {
                "Footprints": "Kyranb\\Footprints\\FootprintsFacade"
            }
        }
    }
}
